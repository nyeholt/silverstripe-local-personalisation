{
    "name": "symbiote/silverstripe-local-personalisation",
    "description": "Builds a profile of a site user in their browser, to be used for content personalisation",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "personalisation",
        "profile"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/multivaluefield": "^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\Personalisation\\": "src/",
            "Symbiote\\Personalisation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
          "app/dist"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
